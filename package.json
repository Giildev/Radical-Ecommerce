{
  "name": "radical-server",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "i-concurrently" : "npm i -g concurrently && npm i concurrently",
    "iAll": "npm run i-concurrently && concurrently --kill-others-on-fail \"npm i\" \"npm i\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "nodemon": "^1.17.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
